function indexOf(arr, value) {
  if (arr.indexOf(value) < 0) {
     
      return false;
  } else {
      return true;
  }
};
function findplayer(arr,value){
   for(var i=0;i<arr.length;i++){
       if(arr[i].id==value)
           return true;
   }
   return false;
};
module.exports.indexOf = indexOf;
module.exports.findplayer= findplayer;


